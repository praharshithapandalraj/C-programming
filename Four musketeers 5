#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int x1,y1,x2,y2,x3,y3;
    scanf("%d%d%d%d%d%d",&x1,&y1,&x2,&y2,&x3,&y3);
    float x=(x1+x2+x3);
    x=x/3;
    float y=(y1+y2+y3);
    y=y/3;
    printf("%.1f\n",x);
    printf("%.1f",y);
    return 0;
}
